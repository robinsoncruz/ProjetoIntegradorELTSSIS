                <property
                  dataType="System.String"
                  description="O comando SQL a ser executado."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	CLIENTE.IDCLIENTE,
	NOME,
	DTCADASTRO,
	CASE WHEN TIPO='F' THEN 'FÍSICO' ELSE 'JURÍDICO' END TIPO,
	ENDERECO.Bairro, 
	ENDERECO.Cidade,
	ENDERECO.UF,
	ENDERECO.Pais PAIS,
	PESSOAFISICA.EstadoCivil AS ESTADOCIVIL,
	CASE WHEN PESSOAFISICA.Sexo = 'F' THEN 'FEMININO'
		 WHEN PESSOAFISICA.Sexo = 'M' THEN 'MASCULINO' END SEXO  
FROM CLIENTE 
INNER JOIN ENDERECOCLIENTE 
ON CLIENTE.IdCliente = ENDERECOCLIENTE.IdCliente 
INNER JOIN ENDERECO 
ON ENDERECO.IdEndereco = ENDERECOCLIENTE.IdEndereco 
LEFT JOIN PESSOAFISICA
ON CLIENTE.IdCliente = PESSOAFISICA.IdCliente</property>
